#include <stdio.h>
#include <locale.h>


int main(){

setlocale(LC_ALL, "Portuguese");
float media, frequencia;
printf("media");
scanf("%f",&media);
printf("frequencia");
scanf("%f",&frequencia);
if (media>=6.0 && frequencia>=75){
          printf("aprovado!");

}else{

    printf("reprovado");
}
return 0;
}
