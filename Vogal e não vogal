#include <stdio.h>
#include <locale.h>

int main(){
setlocale(LC_ALL, "Portuguese");
   char letra;
   printf("Digite uma letra");
   scanf ("%c",&letra);
   if(letra=='a'||letra=='e'||letra=='i'||letra=='o'||letra=='u'){
       printf("é vogal!");
   }else{
       printf("não é vogal");
   }


   return 0;
  }
